    },
    "OrderItemStreamId": {
      "type": "string",
      "maxLength": 150,
      "$comment": "cip88:ref:ceramic://kjzl6cwe1jw14a75ja45rmk9voad56oo652heuis07hw5ja0a7uyih39kivv8ed"
    },
    "ProductStreamId": {
      "type": "string",
      "maxLength": 150,
