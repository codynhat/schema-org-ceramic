    },
    "ImageObjectStreamId": {
      "type": "string",
      "maxLength": 150,
      "$comment": "cip88:ref:ceramic://kjzl6cwe1jw148lzzpczw20nuk4tli92hzn3fgpp3fsa2df6cd1djeqrpyntinw"
    },
    "CreativeWorkStreamId": {
      "type": "string",
      "maxLength": 150,
      "$comment": "cip88:ref:ceramic://kjzl6cwe1jw147r2mldf081wbv77nkeqi1kvnzqit3di2kym56sdvq797wlibww"
    },
    "ActionStreamId": {
