{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "OrderItem",
  "description": "An order item is a line of an order. It includes the quantity and shipping details of a bought offer.",
  "type": "object",
  "allOf": [
    {
      "description": "A utility class that serves as the umbrella for a number of 'intangible' things such as quantities, structured values, etc.",
      "$ref": "#/definitions/Intangible"
    }
  ],
  "properties": {
    "orderDelivery": {
      "description": "The delivery of the parcel related to this order or order item.",
      "$ref": "#/definitions/ParcelDeliveryStreamId"
    },
    "orderItemNumber": {
      "description": "The identifier of the order item.",
      "type": "string"
    },
    "orderItemStatus": {
      "description": "The current status of the order item.",
      "$ref": "#/definitions/OrderStatusStreamId"
    },
    "orderQuantity": {
      "description": "The number of the item ordered. If the property is not set, assume the quantity is one.",
      "type": "number"
    },
    "orderedItem": {
      "description": "The item ordered.",
      "oneOf": [
        {
          "anyOf": [
            { "$ref": "#/definitions/OrderItemStreamId" },
            { "$ref": "#/definitions/ProductStreamId" },
            { "$ref": "#/definitions/ServiceStreamId" }
          ]
        },
        {
          "type": "array",
          "items": {
            "anyOf": [
              { "$ref": "#/definitions/OrderItemStreamId" },
              { "$ref": "#/definitions/ProductStreamId" },
              { "$ref": "#/definitions/ServiceStreamId" }
            ]
          }
        }
      ]
    }
  },
  "definitions": {
    "Intangible": {
      "title": "Intangible",
      "description": "A utility class that serves as the umbrella for a number of 'intangible' things such as quantities, structured values, etc.",
      "type": "object",
      "allOf": [
        {
          "description": "The most generic type of item.",
          "$ref": "#/definitions/Thing"
        }
      ]
    },
    "Thing": {
      "title": "Thing",
      "description": "The most generic type of item.",
      "type": "object",
      "properties": {
        "@context": { "type": "string" },
        "@type": { "type": "string" },
        "additionalType": {
          "description": "An additional type for the item, typically used for adding more specific types from external vocabularies in microdata syntax. This is a relationship between something and a class that the thing is in. In RDFa syntax, it is better to use the native RDFa syntax - the 'typeof' attribute - for multiple types. Schema.org tools may have only weaker understanding of extra types, in particular those defined externally.",
          "oneOf": [
            { "type": "string", "format": "uri" },
            { "type": "array", "items": { "type": "string", "format": "uri" } }
          ]
        },
        "alternateName": {
          "description": "An alias for the item.",
          "oneOf": [
            { "type": "string" },
            { "type": "array", "items": { "type": "string" } }
          ]
        },
        "description": {
          "description": "A description of the item.",
          "oneOf": [
            { "type": "string" },
            { "type": "array", "items": { "type": "string" } }
          ]
        },
        "disambiguatingDescription": {
          "description": "A sub property of description. A short description of the item used to disambiguate from other, similar items. Information from other properties (in particular, name) may be necessary for the description to be useful for disambiguation.",
          "oneOf": [
            { "type": "string" },
            { "type": "array", "items": { "type": "string" } }
          ]
        },
        "identifier": {
          "description": "The identifier property represents any kind of identifier for any kind of [[Thing]], such as ISBNs, GTIN codes, UUIDs etc. Schema.org provides dedicated properties for representing many of these, either as textual strings or as URL (URI) links. See [background notes](/docs/datamodel.html#identifierBg) for more details.\n        ",
          "anyOf": [
            { "type": "string", "format": "uri" },
            { "type": "string" },
            { "$ref": "#/definitions/PropertyValueStreamId" }
          ]
        },
        "image": {
          "description": "An image of the item. This can be a [[URL]] or a fully described [[ImageObject]].",
          "oneOf": [
            {
              "anyOf": [
                { "type": "string", "format": "uri" },
                { "$ref": "#/definitions/ImageObjectStreamId" }
              ]
            },
            {
              "type": "array",
              "items": {
                "anyOf": [
                  { "type": "string", "format": "uri" },
                  { "$ref": "#/definitions/ImageObjectStreamId" }
                ]
              }
            }
          ]
        },
        "mainEntityOfPage": {
          "description": "Indicates a page (or other CreativeWork) for which this thing is the main entity being described. See [background notes](/docs/datamodel.html#mainEntityBackground) for details.",
          "oneOf": [
            {
              "anyOf": [
                { "type": "string", "format": "uri" },
                { "$ref": "#/definitions/CreativeWorkStreamId" }
              ]
            },
            {
              "type": "array",
              "items": {
                "anyOf": [
                  { "type": "string", "format": "uri" },
                  { "$ref": "#/definitions/CreativeWorkStreamId" }
                ]
              }
            }
          ]
        },
        "name": { "description": "The name of the item.", "type": "string" },
        "potentialAction": {
          "description": "Indicates a potential Action, which describes an idealized action in which this thing would play an 'object' role.",
          "oneOf": [
            { "$ref": "#/definitions/ActionStreamId" },
            {
              "type": "array",
              "items": { "$ref": "#/definitions/ActionStreamId" }
            }
          ]
        },
        "sameAs": {
          "description": "URL of a reference Web page that unambiguously indicates the item's identity. E.g. the URL of the item's Wikipedia page, Wikidata entry, or official website.",
          "oneOf": [
            { "type": "string", "format": "uri" },
            { "type": "array", "items": { "type": "string", "format": "uri" } }
          ]
        },
        "subjectOf": {
          "description": "A CreativeWork or Event about this Thing.",
          "oneOf": [
            {
              "anyOf": [
                { "$ref": "#/definitions/CreativeWorkStreamId" },
                { "$ref": "#/definitions/EventStreamId" }
              ]
            },
            {
              "type": "array",
              "items": {
                "anyOf": [
                  { "$ref": "#/definitions/CreativeWorkStreamId" },
                  { "$ref": "#/definitions/EventStreamId" }
                ]
              }
            }
          ]
        },
        "url": {
          "description": "URL of the item.",
          "type": "string",
          "format": "uri"
        }
      }
    },
    "ParcelDeliveryStreamId": {
      "type": "string",
      "maxLength": 150,
      "$comment": "cip88:ref:ceramic://kjzl6cwe1jw149qqxdqkxpqvum3cc4s8mz340f3y90xom1usoga9qrhtt33og0z"
    },
    "OrderStatusStreamId": {
